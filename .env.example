OPENAI_API_KEY=
TAVILY_API_KEY=
DOC_PATH=./my-docs

# Search Engine API Keys
SERPER_API_KEY=

# Serper Search Configuration
# Geographic location (country code): us, kr, jp, de, fr, etc.
SERPER_REGION=us
# Language code: en, ko, ja, de, fr, etc.
SERPER_LANGUAGE=en
# Time range filter: qdr:h (hour), qdr:d (day), qdr:w (week), qdr:m (month), qdr:y (year)
# You can also specify numbers: qdr:h12 (12 hours), qdr:d3 (3 days), qdr:w2 (2 weeks), etc.
SERPER_TIME_RANGE=
# Exclude sites from search results (comma-separated): youtube.com,facebook.com,twitter.com
SERPER_EXCLUDE_SITES=youtube.com

# NEXT_PUBLIC_GPTR_API_URL=http://0.0.0.0:8000  # Defaults to localhost:8000 if not set